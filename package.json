    "ws": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "circular-dependency-plugin": "^3.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
